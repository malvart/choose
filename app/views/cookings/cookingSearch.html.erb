<%= render partial: "header" %>

<div class='search-result'>
  <div class='result-heading'>検 索 結 果</div>

  <div class='category-name-box'>
    <% @result.each do |r| %>
    <%= link_to cooking_path(r.id), method: :get, class: "show-link" do %>
    <div class='category-name'>
      <%= r.cooking_name %>
    </div>
    <% end %>
    <% end %>
  </div>

</div>

<%= render partial: "footer" %>